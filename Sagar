#include<stdio.h>
int main()
{
	int year;
	printf("Ente a year:");
	scanf("%d",&year);
	if(year %400==0){
		("%d is a leap year.",year);}
	else if (year % 100==0){
		printf("%d is not a leap year.",year);}
	else if (year% 4==0){
		printf("%d ia a leap year",year);}
	else{
		printf("%d is not a leap year",year);}
	return 0;
}
		
		
	 	
	

